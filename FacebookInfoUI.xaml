<Window x:Class="ICT365_A2_Andreas_Lau.FacebookInfoUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ICT365_A2_Andreas_Lau"
        mc:Ignorable="d"
        Title="FacebookInfoUI" Height="768" Width="1024">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="254"/>
            <ColumnDefinition Width="0"/>
        </Grid.ColumnDefinitions>
        <TextBlock Name="TBInfo" Grid.Column="0" FontSize="20" FontFamily="Segoe UI Light" Foreground="DodgerBlue" Margin="27,170,0,0"/>
        <Image Name="ProfilePicture" HorizontalAlignment="Left" Height="138" Margin="27,2,0,0" VerticalAlignment="Top" Width="162"/>
    </Grid>
</Window>
